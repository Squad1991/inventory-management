    "lint:fix": "eslint 'src/**/*.{js,ts,tsx,snap}' --fix --no-error-on-unmatched-pattern",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "type-check": "tsc --noEmit",
    "ci": "yarn type-check && yarn test && yarn lint && yarn build",
    "prepare": "husky",
    "clean": "rm -rf dist && rm -rf .parcel-cache && rm -rf node_modules"
  },
