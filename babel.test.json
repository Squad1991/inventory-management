{
  "plugins": [
    ["@compiled/babel-plugin", { "importReact": false, "parserBabelPlugins": ["typescript"] }]
  ],
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    "@babel/preset-typescript",
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "@compiled/babel-plugin",
          {
            "importReact": false,
            "optimizeCss": false
          }
        ]
      ]
    }
  }
}
